SELECT t.fk_tuser_id, u.tuser_id, u.tuser_firstname,u.tuser_firstname, te.team_name FROM tuser u 
	JOIN tuser_team t 
	ON t.fk_tuser_id = u.tuser_id 
	JOIN team te
	ON te.team_id = t.fk_team_id
	WHERE te.team_name = 'TeamA'

