SELECT u.tuser_firstname, t.tournament_name, s.sponsor_name FROM tournament_sponsor ts
	JOIN sponsor s
	ON ts.fk_sponsor_id = s.sponsor_id
	JOIN tournament T 
	ON ts.fk_tournament_id = t.tournament_id
	JOIN tuser u 
	ON t.fk_tuser_id = u.tuser_id




