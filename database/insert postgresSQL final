SELECT DISTINCT u.tuser_firstname, g.game_account_login, gm.game_name FROM game_account_game ga 
	JOIN game_account g 
	ON g.game_account_id = ga.fk_game_account_id
	JOIN game gm 
	ON ga.fk_game_id = gm.game_id
	JOIN tuser u
	ON u.tuser_id = g.fk_tuser_id

